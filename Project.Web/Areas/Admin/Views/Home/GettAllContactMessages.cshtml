
@{
    ViewData["Title"] = "GettAllContactMessages";
}
@model List<NeedHelpViewModel>
<div class="row">
    <div class="table table-striped-columns col-md-6">
        <h1>Destek Talepleri</h1>
        <table class="table">
            <thead>
                <tr>
                    
                    <th>Talep Yapılan Email</th>
                    <th>Talep</th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var info in Model)
                {
                    <tr>
                        <td>@info.Email</td>
                        <td>@info.Text</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

